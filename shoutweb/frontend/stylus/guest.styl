
.container-fluid
  padding: 0px

  
.navbar
  margin-bottom: 0px
    

#contact
  background: rgba(155, 100, 255, 0.6)
  width:100vw
  height:100vh
  // opacity: 0.25
  position: relative
  z-index: 1
  overflow: hidden
  display: block
  
  #modaltest
    width: 33%
    height: 200px
    position: absolute
    top: 200px
    left: 33%
    z-index: 10000
    background: white
    opacity: 0.99
    box-shadow: 0.5em 0.5em 1em #333;
    

#new-main-header
  text-align: center
  font-family: 'Poiret One', cursive
  font-size: 58px

.login-pages
  text-align: center
  transition: all .4s ease-in-out
  background-color: cyan
  // margin-bottom:0
  border-radius:none
  width: 100vw
  height: 100vh
  
  a 
    color: black

  #login-container-box
    width: 500px
    height: 350px
    background: white
    border: 1px solid gray
    margin:auto
    // margin-top: 50px
    

